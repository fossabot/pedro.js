{
  "display": "Node 16",
  "compilerOptions": {
    "lib": [
      "es2020"
    ],
    "module": "commonjs",
    "target": "es2020",
    "sourceMap": true,
    "removeComments": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "strict": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    ".cicd",
    "conf",
    "devenv",
    "dist",
    "doc",
    "node_modules",
    "test"
  ]
}
